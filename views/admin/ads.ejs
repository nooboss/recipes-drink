<!-- partial:partials/header -->
<%- include('../layouts/header.ejs') %>

<!--begin::App-->
<div class="d-flex flex-column flex-root app-root" id="kt_app_root">

  <!--begin::Page-->
  <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
    <!--begin::navbar-->
    <div id="kt_app_header" class="app-header d-flex flex-column flex-stack">
      <!--begin::navbar main-->
      <%- include('../layouts/navbar.ejs') %>
      <!--end::navbar main-->
      <!--begin::Separator-->
      <div class="app-header-separator"></div>
      <!--end::Separator-->
    </div>
    <!--end::navbar-->

    <!--begin::Wrapper-->
    <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
      <!--begin::Sidebar-->
      <div id="kt_app_sidebar" class="app-sidebar flex-column" data-kt-drawer="true" data-kt-drawer-name="app-sidebar" data-kt-drawer-activate="{default: true, lg: false}" data-kt-drawer-overlay="true" data-kt-drawer-width="250px" data-kt-drawer-direction="start" data-kt-drawer-toggle="#kt_app_sidebar_mobile_toggle">
        <!--begin::Main-->
        <%- include('../layouts/sidebar.ejs') %>
        <!--end::Main-->
      </div>
      <!--end::Sidebar-->

      <!--begin::Main-->
      <div class="app-main flex-column flex-row-fluid" id="kt_app_main">
        <!--begin::Content wrapper-->
        <div class="d-flex flex-column flex-column-fluid">
          <!--begin::Toolbar-->
          <div id="kt_app_toolbar" class="app-toolbar pt-5">
            <!--begin::Toolbar container-->
            <div id="kt_app_toolbar_container" class="app-container container-fluid d-flex align-items-stretch">
              <!--begin::Toolbar wrapper-->
              <div class="app-toolbar-wrapper d-flex flex-stack flex-wrap gap-4 w-100">
                <!--begin::Page title-->
                <div class="page-title d-flex flex-column gap-1 me-3 mb-2">
                  <!--begin::Breadcrumb-->
                  <ul class="breadcrumb breadcrumb-separatorless fw-semibold mb-6">
                    <!--begin::Item-->
                    <li class="breadcrumb-item text-gray-700 fw-bold lh-1">
                      <a class="text-gray-500">
                        <i class="ki-solid ki-social-media fs-1 text-gray-400 me-n1"></i>
                      </a>
                    </li>
                    <!--end::Item-->
                    <!--begin::Item-->
                    <li class="breadcrumb-item">
                      <i class="ki-duotone ki-right fs-4 text-gray-700 mx-n1"></i>
                    </li>
                    <!--end::Item-->
                    <!--begin::Item-->
                    <li class="breadcrumb-item text-gray-700 fw-bold lh-1">
                      <a class="text-gray-700 fw-bold">Ads</a>
                    </li>
                    <!--end::Item-->
                    <!--begin::Item-->
                    <li class="breadcrumb-item">
                      <i class="ki-duotone ki-right fs-4 text-gray-700 mx-n1"></i>
                    </li>
                    <!--end::Item-->
                    <!--begin::Item-->
                    <li class="breadcrumb-item text-gray-700 fw-bold lh-1">View Admob</li>
                    <!--end::Item-->
                  </ul>
                  <!--end::Breadcrumb-->
                  <!--begin::Title-->
                  <h1 class="page-heading d-flex flex-column justify-content-center text-dark fw-bolder fs-1 lh-0">AdMob</h1>
                  <!--end::Title-->
                </div>
                <!--end::Page title-->
              </div>
              <!--end::Toolbar wrapper-->
            </div>
            <!--end::Toolbar container-->
          </div>
          <!--end::Toolbar-->

          <!--begin::Content-->
          <div id="kt_app_content" class="app-content flex-column-fluid">
            <!--begin::Content container-->
            <div id="kt_app_content_container" class="app-container container-fluid">

              <!-- begin ::row  -->
              <div class="row">

                <!-- flash message -->
                <% if(flash.error.length> 0){ %>
                <!--begin::Alert-->
                <div class="alert alert-dismissible bg-light-danger d-flex align-items-center mb-10">
                  <!--begin::Content-->
                  <span class="fs-4 text-danger fw-bold text-center ps-5"><%= flash.error %></span>
                  <!--end::Content-->
                  <!--begin::Close-->
                  <button type="button" class="btn btn-icon ms-sm-auto" data-bs-dismiss="alert">
                    <i class="ki-duotone ki-cross fs-1 text-danger fw-bold"><span class="path1"></span><span class="path2"></span></i>
                  </button>
                  <!--end::Close-->
                </div>
                <!--end::Alert-->
                <% } %>
                <!-- complete flash message -->

                <!-- flash message -->
                <% if(flash.success.length> 0){ %>
                <!--begin::Alert-->
                <div class="alert alert-dismissible bg-light-success d-flex align-items-center mb-10">
                  <!--begin::Content-->
                  <span class="fs-4 text-success fw-bold text-center ps-5"><%= flash.success %></span>
                  <!--end::Content-->
                  <!--begin::Close-->
                  <button type="button" class="btn btn-icon ms-sm-auto" data-bs-dismiss="alert">
                    <i class="ki-duotone ki-cross fs-1 text-success fw-bold"><span class="path1"></span><span class="path2"></span></i>
                  </button>
                  <!--end::Close-->
                </div>
                <!--end::Alert-->
                <% } %>
                <!-- complete flash message -->

                <!-- begin::col -->
                <div class="col-xl-6">
                  <!--begin::card-->
                  <div class="card card-flush">

                    <!--begin::Card header-->
                    <div class="card-header border-0 cursor-pointer">

                      <!--begin::Card title-->
                      <div class="card-title m-0">
                        <h3 class="fw-bolder m-0 fs-1">Andriod</h3>
                      </div>
                      <!--end::Card title-->

                    </div>
                    <!--begin::Card header-->

                    <!--begin::Form-->
                    <form method="post" class="form" action="/android-ad-config">

                      <!--begin::Card body-->
                      <div class="card-body border-top p-9">

                        <!--begin::Input group-->
                        <input type="hidden" name="id" value="<%=  adData && adData._id  ? adData._id : '' %>">
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-5">
                          <label class="col-lg-3 required col-form-label fw-semibold fs-6">Andriod</label>
                          <div class="col-lg-9 fv-row">
                            <label class="form-check form-switch form-check-custom form-check-solid">
                              <input class="form-check-input" type="checkbox" value="1" name="android_is_enable" <%= adData && adData.android_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-8">
                          <div class="col-sm-10">
                            <label class="form-label required">App Ad Id </label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="android_app_ad_id" placeholder="Enter app ad id" value="<%= adData && adData.android_app_ad_id ? adData.android_app_ad_id : "" %>" required />
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-8">
                          <div class="col-sm-10">
                            <label class="form-label required">Banner Ad Id </label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="android_banner_ad_id" placeholder="Enter banner ad id" value="<%= adData && adData.android_banner_ad_id ? adData.android_banner_ad_id : "" %>" required />
                          </div>
                          <div class="col-sm-2">
                            <label class="form-check form-switch form-check-custom form-check-solid mt-10">
                              <input class="form-check-input" type="checkbox" value="1" name="android_banner_ad_id_is_enable" <%= adData && adData.android_banner_ad_id_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-8">
                          <div class="col-sm-10">
                            <label class="form-label required">Interstitial Ad Id </label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="android_interstitial_ad_id" placeholder="Enter android interstital ad id" value="<%= adData && adData.android_interstitial_ad_id ? adData.android_interstitial_ad_id : "" %>" required />
                          </div>
                          <div class="col-sm-2">
                            <label class="form-check form-switch form-check-custom form-check-solid mt-10">
                              <input class="form-check-input" type="checkbox" value="1" name="android_interstitial_ad_id_is_enable" <%= adData && adData.android_interstitial_ad_id_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row">
                          <div class="col-sm-10">
                            <label class="form-label required">Rewarded ads </label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="android_rewarded_ads" placeholder="Enter rewared ads" value="<%= adData && adData.android_rewarded_ads ? adData.android_rewarded_ads : "" %>" required />
                          </div>
                          <div class="col-sm-2">
                            <label class="form-check form-switch form-check-custom form-check-solid mt-10">
                              <input class="form-check-input" type="checkbox" value="1" name="android_rewarded_ads_is_enable" <%= adData && adData.android_rewarded_ads_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                      </div>
                      <!--end::Card body-->

                      <!--begin::Actions-->
                      <div class="card-footer d-flex justify-content-start py-6 px-9">
                        <button type="submit" class="btn btn-primary me-5">Save</button>
                      </div>
                      <!--end::Actions-->

                    </form>
                    <!--end::Form-->
                  </div>
                  <!--end::Card body-->
                </div>
                <!--end::col-->

                <!-- begin::col -->
                <div class="col-xl-6">
                  <!--begin::card-->
                  <div class="card card-flush">

                    <!--begin::Card header-->
                    <div class="card-header border-0 cursor-pointer">

                      <!--begin::Card title-->
                      <div class="card-title m-0">
                        <h3 class="fw-bolder m-0 fs-1">ios</h3>
                      </div>
                      <!--end::Card title-->

                    </div>
                    <!--begin::Card header-->

                    <!--begin::Form-->
                    <form method="post" class="form" action="/ios-ad-config">

                      <!--begin::Card body-->
                      <div class="card-body border-top p-9">

                        <!--begin::Input group-->
                        <input type="hidden" name="id" value="<%=  adData && adData._id  ? adData._id : '' %>">
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-5">
                          <label class="col-lg-3 required col-form-label fw-semibold fs-6">ios</label>
                          <div class="col-lg-9 fv-row">
                            <label class="form-check form-switch form-check-custom form-check-solid">
                              <input class="form-check-input" type="checkbox" value="1" name="ios_is_enable" <%= adData && adData.ios_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-8">
                          <div class="col-sm-10">
                            <label class="form-label required">App Ad Id</label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="ios_app_ad_id" placeholder="Enter app ad id" value="<%= adData && adData.ios_app_ad_id ? adData.ios_app_ad_id : "" %>" required />
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-8">
                          <div class="col-sm-10">
                            <label class="form-label required">Banner Ad Id</label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="ios_banner_ad_id" placeholder="Enter banner ad id" value="<%= adData && adData.ios_banner_ad_id ? adData.ios_banner_ad_id : "" %>" required />
                          </div>
                          <div class="col-sm-2">
                            <label class="form-check form-switch form-check-custom form-check-solid mt-10">
                              <input class="form-check-input" type="checkbox" value="1" name="ios_banner_ad_id_is_enable" <%= adData && adData.ios_banner_ad_id_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row mb-8">
                          <div class="col-sm-10">
                            <label class="form-label required">Interstitial Ad Id</label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="ios_interstitial_ad_id" placeholder="Enter interstital ad id" value="<%= adData && adData.ios_interstitial_ad_id ? adData.ios_interstitial_ad_id : "" %>" required />
                          </div>
                          <div class="col-sm-2">
                            <label class="form-check form-switch form-check-custom form-check-solid mt-10">
                              <input class="form-check-input" type="checkbox" value="1" name="ios_interstitial_ad_id_is_enable" <%= adData && adData.ios_interstitial_ad_id_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                        <!--begin::Input group-->
                        <div class="row">
                          <div class="col-sm-10">
                            <label class="form-label required">Reward Video Ads Id</label>
                            <input type="text" class="form-control form-control-lg form-control-solid" name="ios_rewarded_ads" placeholder="Enter rewarded ads" value="<%= adData && adData.ios_rewarded_ads ? adData.ios_rewarded_ads : "" %>" required />
                          </div>
                          <div class="col-sm-2">
                            <label class="form-check form-switch form-check-custom form-check-solid mt-10">
                              <input class="form-check-input" type="checkbox" value="1" name="ios_rewarded_ads_is_enable" <%= adData && adData.ios_rewarded_ads_is_enable == 1 ? 'checked' : '' %>>
                            </label>
                          </div>
                        </div>
                        <!--end::Input group-->

                      </div>
                      <!--end::Card body-->

                      <!--begin::Actions-->
                      <div class="card-footer d-flex justify-content-start py-6 px-9">
                        <button type="submit" class="btn btn-primary me-5">Save</button>
                      </div>
                      <!--end::Actions-->

                    </form>
                    <!--end::Form-->
                  </div>
                  <!--end::Card body-->
                </div>
                <!--end::col-->
              </div>
              <!-- end:row -->

            </div>
            <!--end::Content container-->
          </div>
          <!--end::Content-->

        </div>
        <!--end::Content wrapper-->

        <!--begin::Footer-->
        <div id="kt_app_footer" class="app-footer align-items-center justify-content-center justify-content-md-between flex-column flex-md-row py-3">
          <!--begin::Copyright-->
          <div class="text-dark order-2 order-md-1">
            <span class="text-muted fw-semibold me-1"></span>
            <a  target="_blank" class="text-gray-800 text-hover-primary"></a>
          </div>
          <!--end::Copyright-->
        </div>
        <!--end::Footer-->
      </div>
      <!--end:::Main-->


    </div>
    <!--end::Wrapper-->

  </div>
  <!--end::Page-->
</div>
<!--end::App-->

<!--begin::Scrolltop-->
<div id="kt_scrolltop" class="scrolltop" data-kt-scrolltop="true">
  <i class="ki-duotone ki-arrow-up">
    <span class="path1"></span>
    <span class="path2"></span>
  </i>
</div>
<!--end::Scrolltop-->


<!-- partial:partials/header -->
<%- include('../layouts/footer.ejs') %>